{
  "name": "{{ validator_name | default('polkadot-validator') }}",
  "chain": "{{ chain_name | default('polkadot') }}",
  "base-path": "{{ data_dir }}",
  "validator": true,
  "rpc-cors": "all",
  "prometheus-external": true,
  "prometheus-port": 9615,
  "telemetry-url": "{{ telemetry_url | default('wss://telemetry.polkadot.io/submit/ 0') }}",
  "listen-addr": "/ip4/0.0.0.0/tcp/30333",
  "rpc-port": 9933,
  "ws-port": 9944,
  "unsafe-rpc-external": false,
  "unsafe-ws-external": false,
  "rpc-methods": "Safe",
  "database": "RocksDb",
  "state-cache-size": 0,
  "max-runtime-instances": 8,
  "runtime-cache-size": 2
}